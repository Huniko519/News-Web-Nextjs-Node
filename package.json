{
  "name": "frontend",
  "version": "2.1.30",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode=production --config=webpack.prod.js && next build",
    "lint": "npx eslint .",
    "test": "jest",
    "pre-commit": "bash pre-commit.sh",
    "start": "yarn start:dev:int",
    "start:dev:local": "concurrently 'CHOKIDAR_USEPOLLING=true node server.js' 'webpack --config webpack.dev.js'",
    "start:dev:prod": "concurrently 'CHOKIDAR_USEPOLLING=true yarn start:local:prod' 'webpack --config webpack.dev.js'",
    "start:dev:int": "concurrently 'CHOKIDAR_USEPOLLING=true yarn start:local:int' 'webpack --config webpack.dev.js'",
    "start:local:prod": "node -r dotenv/config server.js dotenv_config_path=./.env.local.inews.co.uk",
    "start:local:int": "node -r dotenv/config server.js dotenv_config_path=./.env.local.inewsint.co.uk",
    "start:prod": "yarn start:prod:cron && yarn start:prod:server",
    "start:prod:server": "NODE_ENV=production node server.js",
    "start:prod:cron": "node cron.js"
  },
  "precommit.silent": true,
  "pre-commit": [
    "pre-commit"
  ],
  "browserslist": [
    "IE 10"
  ],
